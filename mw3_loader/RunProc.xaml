<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="TeknoMW3.RunProc"
	x:Name="Window"
	Title="Game is running"
	Width="300" Height="190" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Closing="Window_Closing" Loaded="Window_Loaded" Icon="/TeknoMW3;component/TKARTFANBNATM.png">

	<Grid x:Name="LayoutRoot">
		<Grid Margin="0,0,0,0">
			<Grid.RowDefinitions>
				<RowDefinition Height="131" />
				<RowDefinition Height="*" />
			</Grid.RowDefinitions>
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="*" />
			</Grid.ColumnDefinitions>
			<Image VerticalAlignment="Top" Source="TKARTFANBNATM.png" Grid.Row="0"/>
			<Label Name="RunningLabel" Content="Game is running" Grid.Row="1" Style="{DynamicResource SimpleLabel}" VerticalAlignment="Center" HorizontalAlignment="Center"/>
		</Grid>
	</Grid>
</Window>